Program.Sub.cmdOK_Click.Start
F.Intrinsic.Control.SetErrorHandler("cmdOK_Click_Err")
F.Intrinsic.Control.ClearErrors

V.Local.sError.Declare(String)
V.Local.sBegDate.Declare(String)
V.Local.sEndDate.Declare(String)
V.Local.sAPText.Declare(String)
V.Local.SPYText.Declare(String)
V.Local.sFileText.Declare(String)

Gui.SS_Form1.cmdOK.Enabled(false)

' will need dates in this format, for both subs.  converting, once, now
F.Intrinsic.String.Format(V.Screen.SS_Form1!dtpBeg.Value,"YYYYMMDD",V.Local.sBegDate)
F.Intrinsic.String.Format(V.Screen.SS_Form1!dtpEnd.Value,"YYYYMMDD",V.Local.sEndDate)

F.Intrinsic.Control.SelectCase(V.Screen.SS_Form1!ddlPmntType.Text)
F.Intrinsic.Control.Case("AP")
	F.Intrinsic.Control.CallSub(Ap_lines,"BEGDATE",V.Local.sBegDate,"ENDDATE",V.Local.sEndDate)
	V.Local.sAPText.Set(V.Args.AP)

F.Intrinsic.Control.Case("PAYROLL")
	F.Intrinsic.Control.CallSub(Payroll_lines,"BEGDATE",V.Local.sBegDate,"ENDDATE",V.Local.sEndDate)
	V.Local.sPYText.Set(V.Args.PY)

F.Intrinsic.Control.Case("ALL CHECKS")
	F.Intrinsic.Control.CallSub(Ap_lines,"BEGDATE",V.Local.sBegDate,"ENDDATE",V.Local.sEndDate)
	V.Local.sAPText.Set(V.Args.AP)
	F.Intrinsic.Control.CallSub(Payroll_lines,"BEGDATE",V.Local.sBegDate,"ENDDATE",V.Local.sEndDate)
	V.Local.sPYText.Set(V.Args.PY)

F.Intrinsic.Control.EndSelect

F.Intrinsic.String.Build("{0}{1}",V.Local.sAPText,V.Local.sPYText,V.Local.sFileText)
F.Intrinsic.Control.If(V.Local.sFileText.Trim,<>,"")
	Gui.SS_Form1..Visible(false)
	F.Intrinsic.Control.CallSub(Writefile,"FILETEXT",V.Local.sFileText)
F.Intrinsic.Control.Else
	F.Intrinsic.UI.Msgbox("No records found for entered parameters.")
	Gui.SS_Form1.cmdOK.Enabled(True)
	F.Intrinsic.Control.ExitSub
F.Intrinsic.Control.EndIf

F.Intrinsic.Control.CallSub(Ss_form1_unload)



F.Intrinsic.Control.ExitSub

F.Intrinsic.Control.Label("cmdOK_Click_Err")
F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
	Function.Intrinsic.String.Concat("Project: GCG_GRE_Positive_Pay-TOB.gas",V.Ambient.Newline,V.Ambient.Newline,"Subroutine: ",V.Ambient.CurrentSubroutine,V.Ambient.NewLine,"Error Occurred ",V.Ambient.ErrorNumber," with description ",V.Ambient.ErrorDescription,V.Local.sError)
	F.Intrinsic.Debug.SetLA(V.Local.sError)
	F.Intrinsic.UI.Msgbox("An error occured during processing.  Please contact Global Shop for Assistance")
Function.Intrinsic.Control.EndIf

Program.Sub.cmdOK_Click.End

