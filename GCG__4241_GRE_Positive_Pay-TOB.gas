		' FILLER
		V.Local.sHold(5).Set("        ")
		' EMPLOYEE NO - left justify,blank fill, should beformatted that way withthe positional string
		F.Intrinsic.String.RPad(V.Local.sHold(6)," ",14,V.Local.sHold(6))
		' PAYEE NAME- left justify,blank fill, should beformatted that way withthe positional string

		F.Intrinsic.String.Join(V.Local.sHold," ",V.Local.sRec)
		F.Intrinsic.String.RPad(V.Local.sRec," ",200,V.Local.sRec)
		F.Intrinsic.String.Build("{0}{1}{2}",v.Local.sPYText,v.Local.sRec,v.Ambient.NewLine,v.Local.sPYText)

	F.Intrinsic.Control.Next(V.Local.iFor)
