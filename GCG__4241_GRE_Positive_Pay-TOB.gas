Program.Sub.AP_Lines.Start
F.Intrinsic.Control.SetErrorHandler("loadDropdown_Err")
F.Intrinsic.Control.ClearErrors

V.Local.sError.Declare(String)
V.Local.sSel.Declare(String)
V.Local.sRet.Declare(String)
V.Local.iFor.Declare(Long)
V.Local.sHold.Declare(String)
V.Local.sAPText.Declare(String)
V.Local.sRec.Declare(String)

' look for all AP checks written within the entered date range
F.Intrinsic.String.Build("select Ck_Or_Void, Acct_No, Ck_No, Amount, Ck_Date, '' as Hold, Vend_No, Payee from Checks_Log where Ck_Date between '{0}' and '{1}' order by Ck_No",V.Args.BEGDATE,V.Args.ENDDATE,V.Local.sSel)
F.ODBC.Connection!Con.ExecuteAndReturn(V.Local.sSel,V.Local.sRet)
' if checks are found, get them into formatted lines
F.Intrinsic.Control.If(V.Local.sRet.Trim,<>,"")
	' if checks are found, write details to a positional line
	F.Intrinsic.String.Split(V.Local.sRet,"#$#",V.Local.sRet)
	F.Intrinsic.Control.For(V.Local.iFor,V.Local.sRet.LBound,V.Local.sRet.UBound,1)
		F.Intrinsic.String.Split(V.Local.sRet(v.Local.iFor),"*!*",V.Local.sHold)
		V.Local.sHold.RedimPreserve(0,7)

		F.Intrinsic.Control.if(V.Local.sHold(0).Trim,=,"C")
			V.Local.sHold(0).set("I")
		F.Intrinsic.Control.EndIf
		' CK_OR_VOID - will be the C or V read from the recordset
		' ACCT_NO - right justified, 0 fill
		F.Intrinsic.String.LPad(V.Local.sHold(1).Trim,"0",20,V.Local.sHold(1))
		' CK_NO - right justify, zero fill
		F.Intrinsic.String.LPad(V.Local.sHold(2).Trim,"0",18,V.Local.sHold(2))
		' AMOUNT - right justify, zero fill, implied decimal, no "-" on negative numbers
		F.Intrinsic.String.Replace(V.Local.sHold(3),"-","",V.Local.sHold(3))
		F.Intrinsic.String.Replace(V.Local.sHold(3).Format*0.00,".","",V.Local.sHold(3))
		'amount
		F.Intrinsic.String.LPad(V.Local.shold(3),"0",18,V.Local.sHold(3))
		' ISSUE DATE - yyyymmdd, should be read from the recordset in this format
		' FILLER
		V.Local.sHold(5).Set("                      ")
'		F.Intrinsic.String.RPad(V.Local.sHold(6)," ",14,V.Local.sHold(6))
		' PAYEE NAME - left justify,blank fill, should beformatted that way withthe positional string
		F.Intrinsic.String.Build("SELECT NAME_VENDOR FROM V_VENDOR_MASTER WHERE VENDOR = '{0}'",V.Local.sHold(6),V.Local.sSel)
		F.ODBC.Connection!Con.OpenLocalRecordsetRO("rst",V.Local.sSel)
		
		F.Intrinsic.Control.If(V.ODBC.con!rst.EOF,<>,True)
			V.Local.sHold(7).Set(V.ODBC.con!rst.FieldValTrim!NAME_VENDOR)
		F.Intrinsic.Control.EndIf
		
		F.ODBC.con!rst.Close
		' VENDOR NO - left justify,blank fill, should beformatted that way withthe positional string
		V.Local.sHold(6).Set("")
		
		F.Intrinsic.String.Join(V.Local.sHold," ",V.Local.sRec)
		F.Intrinsic.String.RPad(V.Local.sRec," ",144,V.Local.sRec)
		F.Intrinsic.String.Build("{0}{1}{2}",V.Local.sAPText,V.Local.sRec,V.Ambient.NewLine,V.Local.sAPText)

	F.Intrinsic.Control.Next(V.Local.iFor)
F.Intrinsic.Control.EndIf

' send line back to calling sub
F.Intrinsic.Variable.AddRV("AP",V.Local.sAPText)



F.Intrinsic.Control.ExitSub

F.Intrinsic.Control.Label("loadDropdown_Err")
F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
	Function.Intrinsic.String.Concat("Project: GCG_GRE_Positive_Pay-TOB.gas",V.Ambient.Newline,V.Ambient.Newline,"Subroutine: ",V.Ambient.CurrentSubroutine,V.Ambient.NewLine,"Error Occurred ",V.Ambient.ErrorNumber," with description ",V.Ambient.ErrorDescription,V.Local.sError)
	F.Intrinsic.Debug.SetLA(V.Local.sError)
	F.Intrinsic.UI.Msgbox("An error occured during processing.  Please contact Global Shop for Assistance")
Function.Intrinsic.Control.EndIf

Program.Sub.AP_Lines.End

